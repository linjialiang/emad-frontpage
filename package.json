{
  "name": "frontpage",
  "version": "0.0.2",
  "description": "wangdoc.com's frontpage",
  "main": "index.js",
  "scripts": {
    "build": "emad-loppo -t wangdoc-frontpage",
    "build-and-commit": "npm run build && npm run commit",
    "commit": "gh-pages --dist dist --dest dist --branch master --add --repo git@github.com:linjialiang/website.git",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Wangdoc",
    "document"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/linjialiang/emad-frontpage.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm update"
    }
  },
  "author": "Lin Jialiang",
  "license": "ISC",
  "devDependencies": {
    "gh-pages": "latest",
    "husky": "^4.3.8",
    "emad-loppo": "latest",
    "emad-loppo-theme-wangdoc-frontpage": "latest"
  }
}
